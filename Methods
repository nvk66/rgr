methods:
Client, Driver, Car, AutoPark

registerTaxiDriver(Driver driver, AutoPark park)
Request: "Name", "LastName", "CarModel", "CarNumber", "CarColour"
Response: Status, Method

registerClient(Client client)
Request: "Login" "PhoneNumber"
Response: status, method

makeNewOrder(Order order, Driver driver, Car car)
Request: "StartLocation", "EndLocation", "Price", "PhoneNumber", "Name", "LastName", "CarModel",
            "CarNumber", "CarColour"
Response: status, method

getAllClientHistory(Client client)
Request: "Login" "PhoneNumber"
Response: "status", "method", List<Order>
